includes:
    - ./vendor/larastan/larastan/extension.neon

parameters:

    paths:
        - app

    # The level 9 is the highest level
    level: 5

    ignoreErrors:

    excludePaths:
        - ./*/*/FileToBeExcluded.php
        - ./*/*/Socialstream/*.php
        - ./*/*/Jetstream/*.php
        - ./*/*/Fortify/*.php

    checkMissingIterableValueType: false
